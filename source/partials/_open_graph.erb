<meta property="og:title" content="<%= page_title(page_data) %>" />
<meta property="og:type" content="website" />
<meta property="og:url" content="<%= data.seo.site_url %><%= current_page.url %>" />
<meta property="og:image" content="<%= data.social.thumbnail_url %>" />
<meta property="og:description" content="<%= page_data.description ||= data.seo.description %>"
